{
  "name": "@confradar/web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "ConfRadar web frontend - AI-powered conference deadline tracker",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "analyze": "vite build --mode production && open dist/stats.html",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["confradar", "conference", "deadline", "tracker"],
  "author": "ConfRadar Team",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-compression": "^0.5.1"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
